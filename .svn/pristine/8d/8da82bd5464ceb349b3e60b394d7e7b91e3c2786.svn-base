package com.adargasystems.concurrent;

import java.awt.Polygon;

/**
 * 
 *
 * @author $Author$
 * @id $Id$
 *
 */
public interface WayPoint {

	/**
	 * 
	 * @return
	 */
	Double getLatitude();

	/**
	 * 
	 * @return
	 */
	Double getLongitude();

	/**
	 * @return the altitude
	 */
	Double getAltitude();

	default boolean isInPolygon(final Polygon polygon) {
		return true;
	}

}